<% if @resource.valid? %>
  <%= turbo_stream.replace "like_posts_user_#{@posts_user}" do %>
    <%= turbo_frame_tag "like_posts_user_#{@posts_user}" do %>
      <%= render partial: "posts/users/likes/dislike", locals: { resource: @resource, posts_user: @posts_user } %>
    <% end %>
  <% end %>
<% else %>
  <%= turbo_stream.append "flash" do %>
    <%= render partial: "layouts/shared/flash", locals: { flash: { error: ""} } %>
  <% end %>
<% end %>